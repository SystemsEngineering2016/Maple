<body>
<div class = "jumbotron">
  <h1 align="center">Edit this Version</h1>
  <%= form_for :version, url: version_path(@version), method: :patch do |f| %>
  <% if @version.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@version.errors.count, "error") %> prohibited
      this article from being saved:
    </h2>
    <ul>
      <% @version.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>

  <p align="center">
    <%= f.label :Version_Name %><br>
    <%= f.text_field :Version_Name %>
  </p>
  <br>
  <p align="center">
    <%= f.submit %>
  </p>

<% end %>
</div>
</body>
